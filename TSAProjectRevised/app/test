var obj1 = {
  //method1: function() {
  //  return "foo"
  //},
  method2: function() {
    return "bar";
  }
}

var obj2 = {
  //mehtod33: function() {
  //  return "foo"
  //}
}

objectToInjectInto = function(injectable) {
  return {
    method1: function() {
      return "foo"
    }.merge(injectable)
  }
}

newObj = objectToInjectInto(obj1)

newObj.mehtod1 => foo
newObj.method2 => bar

newObj2 = objectToInjectInto(obj2)

newObj2.method1 => foo
newObj2.mehtod2 => undefined

